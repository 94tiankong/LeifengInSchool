.fb-type{
  background: #fff;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding-left: 30rpx;
  padding-right: 30rpx;
}

.fb-type .type-label{
  height: 36rpx;
  flex: 1;
  color: #333;
  font-size: 28rpx;
  padding-right:50rpx;
}

.fb-type .type-icon{
  height: 36rpx;
  width: 36rpx;
}


/* 弹框 */
.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0,0,0,.2);
  z-index: 9000;
  opacity: 0.7;
}
 
.dialog-box {
  width: 580rpx;
  height: 465rpx;
  position: fixed;
  top: 50%;
  left: 0;
  z-index: 9999;
  margin: -370rpx 85rpx;
  background-color: #fff;
  border-radius: 36rpx;
  display: flex;
  flex-direction: column;
}
 
.dialog-box image {
  width: 80rpx;
  height:80rpx;
  position: absolute;
  top:10rpx;
  right:10rpx;
}
